<%if(success.length>0){%>
    <div class="row">
        <div class="alert alert-success alert-dismissible fade show offset-3 col-6" role="alert">
            <%=success%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    
           </div>
    </div>
    
<%}%>

<%if(error.length>0){%>
    <div class="row">
        <div class="alert alert-danger alert-dismissible fade show offset-3 col-6" role="alert">
            <%=error%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    
           </div>
    </div>
    
<%}%>